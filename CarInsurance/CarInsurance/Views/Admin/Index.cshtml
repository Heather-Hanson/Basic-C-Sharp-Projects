@model IEnumerable<CarInsurance.View_Models.InsureeVm>


@{
  ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
  @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
  <tr>
    <th>
      First Name
    </th>
    <th>
      Last Name
    </th>
    <th>
      Email Address
    </th>
<th>
  Quote
</th>
    <th>Action</th>
  </tr>

  @foreach (var item in Model)
  {
    <tr>
      <td>
        @item.FirstName
      </td>
      <td>
        @item.LastName
      </td>
      <td>
        @item.EmailAddress
      </td>
 
      <td>
        @item.Quote
      </td>
      <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id})  |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
      </td>
    </tr>
  }

</table>
